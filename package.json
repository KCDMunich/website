{
  "name": "gatsby-tailwind-starter",
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Alex Barashkov <alex@pixelpoint.io>",
  "engines": {
    "node": ">=20.5.1",
    "npm": ">=v9.8.0"
  },
  "scripts": {
    "start": "NODE_NO_WARNINGS=1 npm run develop",
    "develop": "gatsby develop -H 0.0.0.0",
    "build": "NODE_NO_WARNINGS=1 gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "format": "prettier --write --ignore-path .gitignore .",
    "lint": "npm run lint:js & npm run lint:md",
    "lint:fix": "npm run lint:js:fix & npm run lint:md:fix",
    "lint:js": "eslint --ext .js,.jsx --ignore-path .gitignore .",
    "lint:js:fix": "eslint --fix --ext .js,.jsx --ignore-path .gitignore .",
    "lint:md": "markdownlint --ignore-path .gitignore .",
    "lint:md:fix": "markdownlint --fix --ignore-path .gitignore ."
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-brands-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.1",
    "@fullcalendar/core": "^6.1.19",
    "@fullcalendar/daygrid": "^6.1.19",
    "@fullcalendar/react": "^6.1.19",
    "@fullcalendar/timegrid": "^6.1.19",
    "@mdx-js/react": "^2.3.0",
    "@mui/material": "^7.3.6",
    "axios": "^1.13.2",
    "clsx": "^2.1.1",
    "dompurify": "^3.3.1",
    "framer-motion": "^12.23.26",
    "gatsby-plugin-image": "^3.15.0",
    "jsdom": "^27.3.0",
    "lodash": "^4.17.21",
    "lodash.get": "^4.4.2",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.561.0",
    "path": "^0.12.7",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-card-flip": "^1.2.3",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^5.5.0",
    "react-social-icons": "^6.25.0",
    "react-tooltip": "^5.30.0",
    "remark-gfm": "^4.0.1",
    "slugify": "^1.6.6",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@svgr/webpack": "^8.1.0",
    "autoprefixer": "^10.4.23",
    "babel-loader": "^10.0.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "gatsby": "^5.15.0",
    "gatsby-alias-imports": "^1.0.6",
    "gatsby-plugin-canonical-urls": "^5.15.0",
    "gatsby-plugin-manifest": "^5.15.0",
    "gatsby-plugin-mdx": "^5.15.0",
    "gatsby-plugin-postcss": "^6.15.0",
    "gatsby-plugin-sharp": "^5.15.0",
    "gatsby-plugin-sitemap": "^6.15.0",
    "gatsby-plugin-svgr-svgo": "^2.0.0",
    "gatsby-source-filesystem": "^5.15.0",
    "gatsby-transformer-sharp": "^5.15.0",
    "lint-staged": "^16.2.7",
    "markdownlint-cli": "^0.47.0",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2"
  },
  "overrides": {
    "cookie": "0.7.2",
    "tmp": "0.2.5"
  },
  "keywords": [
    "react",
    "gatsby",
    "tailwind"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pixel-point/gatsby-tailwind-starter"
  },
  "bugs": {
    "url": "https://github.com/pixel-point/gatsby-tailwind-starter/issues"
  }
}
